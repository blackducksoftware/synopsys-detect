<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss z} %-5level [%thread] --- %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="httpclient" level="WARN"/>
    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
